<?xml version="1.0" encoding="utf-8"?>
<com.getbase.floatingactionbutton.FloatingActionsMenu xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fab_menu"
    style="@style/fam_style"
    android:layout_gravity="bottom|end"
    fab:fab_labelsPosition="left" >

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_share_wall"
        style="@style/fab_style"
        fab:fab_colorNormal="@color/biru"
        fab:fab_icon="@drawable/ic_fab_fb_wall"
        fab:fab_title="@string/share.wall" />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_share_grup"
        style="@style/fab_style"
        fab:fab_colorNormal="@color/biru"
        fab:fab_icon="@drawable/ic_fab_fb_grup"
        fab:fab_title="@string/share.grup" />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_share_app"
        style="@style/fab_style"
        fab:fab_colorNormal="@color/ijo"
        fab:fab_icon="@drawable/ic_fab_app"
        fab:fab_title="@string/share.app" />

</com.getbase.floatingactionbutton.FloatingActionsMenu>